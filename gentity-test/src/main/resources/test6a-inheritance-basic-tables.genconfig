<mappingConfig
	xmlns='https://github.com/gentity/gentity/ns'
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	    https://github.com/gentity/gentity/ns
			../../../../gentity-core/src/main/java/com/github/gentity/core/xsd/genconfig.xsd
	"
	
	>
	<configuration
		extends='com.github.gentity.test.test6a_inheritance_basic_tables.GeneralBase'
	/>
	
	<!--
	NOTE: We won't declare entityTable for PERSON; it'll be generated by default,
	and extend the 'GeneralBase' class defined above
	-->
	
	<!--
	NOTE: For table 'CAR', we override the default extends with a local setting
	-->
	<entityTable table='CAR' extends='com.github.gentity.test.test6a_inheritance_basic_tables.VehicleBase'/>
</mappingConfig>